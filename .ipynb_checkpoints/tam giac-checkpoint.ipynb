    "# tính diện tích hình tam giác biết kích thước 3 cạnh\n",
    "a = float(input(\"nhập cạnh a = \"))\n",
    "b = float(input(\"nhập cạnh b = \"))\n",
    "c = float(input(\"nhập cạnh c = \"))\n",
    "# tính nửa chu vi P\n",
    "p = (a + b + c)/2\n",
    "s = (p*(p-a)*(p-b)*(p-c))**0.5\n",
    "print(\"diện tích s =\", s)"
   ]
